class Solution:
    def mergeKLists(self, l: List[ListNode]) -> ListNode:
        
        b=[]
        for i in l:
            k=i
            while k:
                b.append(k.val)
                k=k.next
        if len(b)<1:
            return
        b.sort()
        n=ListNode(b[0])
        e=n
        for i in range(1,len(b)):
            e.next=ListNode(b[i])
            e=e.next
        return n
